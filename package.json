{
  "name": "clulib",
  "version": "2.0.0",
  "description": "closure-library utilities",
  "homepage": "https://github.com/b-strauss/clulib",
  "bugs": "https://github.com/b-strauss/clulib/issues",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/b-strauss/clulib.git"
  },
  "keywords": [
    "javascript",
    "library",
    "closure",
    "es6",
    "es2015"
  ],
  "scripts": {
    "compile": "./node_modules/.bin/gulp compile",
    "compile-debug": "./node_modules/.bin/gulp compile --debug=true",
    "deps": "./node_modules/.bin/gulp deps",
    "lint": "./node_modules/.bin/eslint \"./src/**\" \"./test/**\" browsers.conf.js gulpfile.js karma.conf.js test_main.js",
    "test": "./node_modules/.bin/karma start karma.conf.js"
  },
  "pre-commit": [
    "lint"
  ],
  "devDependencies": {
    "eslint": "4.3.0",
    "google-closure-compiler": "20170626.0.0",
    "google-closure-library": "20170626.0.0",
    "gulp": "3.9.1",
    "gulp-sourcemaps": "2.6.0",
    "gulp-util": "3.0.8",
    "jasmine": "2.7.0",
    "jasmine-core": "2.7.0",
    "karma": "1.7.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-jasmine": "1.1.0",
    "karma-sauce-launcher": "1.1.0",
    "karma-sourcemap-loader": "0.3.7",
    "karma-spec-reporter": "0.0.31",
    "pre-commit": "1.2.2"
  }
}
